    <script type="text/javascript" src="/assets/js/jcanvas.min.js"></script>
    <script type="text/javascript">
        //Each type of test has the ans choices in different places so a different method for each type of test: a,b,c,d,e

        //Choice placement for Images in the A catetory, these are preset since they don't change in the first set of images.
        var startingXA = 115;
        var startingYA = 370;
        var xSpacingA = 125;
        var ySpacingA = 115;
        var widthA = 115;
        var heightA = 110;
        var rowA = 2;
        var columnA = 3;
        var answers = {};

        function setBoardA($canvas, input, imagePath) {
            $canvas.drawImage({
                layer: true,
                x: 0, y: 0,
                index: 0,
                source: imagePath,
                width: 600, height: 600

            }).drawLayers();

            var count = 1;
            var answer = 1;

            for (var y = 0; y < rowA; y++) {
                for (var x = 0; x < columnA; x++) {
                    $canvas.addLayer({
                        type: 'rectangle',
                        groups: ['questions'],
                        name: 'step' + count++,
                        strokeStyle: 'red',
                        strokeWidth: 0,
                        x: startingXA + (xSpacingA * x), y: startingYA + (ySpacingA * y),
                        width: widthA,
                        height: heightA,
                        data: {answer:answer},
                        click: function (layer) {
                            answers[input] = parseInt(layer.data.answer);
                            $('input[name="' + input + '"]').val(layer.name.replace('step', ''));

                            $canvas.setLayerGroup('questions', {
                                strokeWidth: 0
                            }).drawLayers();

                            $canvas.setLayer(layer.name, {
                                strokeWidth: 4
                            }).drawLayers();
                        }
                    }).drawLayers();

                    answer++;
                }
            }
        }


        // One function where the different variables are just passed in.
        function setBoard($canvas, input, imagePath, startingX, startingY, xSpacing, ySpacing, width, height, row, column) {
            //Choice placement for Images in the B category, 6-9
            var startingX = startingX;
            var startingY = startingY;
            var xSpacing = xSpacing;
            var ySpacing = ySpacing;
            var width = width;
            var height = height;
            var row = row;
            var column = column;
            $canvas.drawImage({
                layer: true,
                x: 0, y: 0,
                index: 0,
                source: imagePath,
                width: 600, height: 600
            }).drawLayers();

            var count = 1;
            var answer = 1;

            for (var y = 0; y < row; y++) {
                for (var x = 0; x < column; x++) {
                    $canvas.addLayer({
                        type: 'rectangle',
                        groups: ['questions'],
                        name: 'step' + count++,
                        strokeStyle: 'red',
                        strokeWidth: 0,
                        x: startingX + (xSpacing * x), y: startingY + (ySpacing * y),
                        width: width,
                        height: height,
                        data: {answer:answer},
                        click: function (layer) {
                            answers[input] = parseInt(layer.data.answer);
                            $('input[name="' + input + '"]').val(layer.name.replace('step', ''));

                            $canvas.setLayerGroup('questions', {
                                strokeWidth: 0
                            }).drawLayers();

                            $canvas.setLayer(layer.name, {
                                strokeWidth: 4
                            }).drawLayers();
                        }
                    }).drawLayers();

                    answer++;
                }
            }
        }

